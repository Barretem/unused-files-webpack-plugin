{
  "name": "unused-files-webpack-plugin",
  "version": "3.4.0",
  "description": "Glob all files that are not compiled by webpack under webpack's context",
  "license": "MIT",
  "author": {
    "name": "tomchentw",
    "email": "developer@tomchentw.com",
    "url": "https://github.com/tomchentw"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomchentw/unused-files-webpack-plugin"
  },
  "bugs": {
    "url": "https://github.com/tomchentw/unused-files-webpack-plugin/issues"
  },
  "homepage": "https://github.com/tomchentw/unused-files-webpack-plugin/",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "src/",
    "CHANGELOG.md"
  ],
  "keywords": [
    "unused",
    "redundancy",
    "files",
    "require",
    "webpack",
    "webpack-plugin"
  ],
  "scripts": {
    "test": "jest",
    "prebuild:lib": "rimraf lib",
    "build:lib": "babel --out-dir lib 'src/**/*.js'",
    "precommit:lib": "npm run build:lib",
    "commit:lib": "git add -A && git commit -m 'chore(lib): compile from src with `babel-multi-env`'",
    "prerelease": "npm run commit:lib",
    "release": "standard-version -m 'chore(release): %s \n\n* CHANGELOG: https://github.com/tomchentw/unused-files-webpack-plugin/blob/v%s/CHANGELOG.md'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 8
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "\\d+\\.\\d+\\.\\d+"
    ]
  },
  "dependencies": {
    "glob-all": "^3.1.0",
    "invariant": "^2.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "memory-fs": "^0.4.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "standard-version": "^4.3.0",
    "webpack": "^3.8.1"
  }
}
