{
  "name": "@barretter/remove-unused-files-webpack-plugin",
  "version": "4.0.1",
  "description": "Glob all files that are not compiled by webpack under webpack's context",
  "license": "MIT",
  "author": {
    "name": "barretem",
    "email": "1178310248@qq.com",
    "url": "https://github.com/Barretem"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Barretem/unused-files-webpack-plugin"
  },
  "bugs": {
    "url": "https://github.com/Barretem/unused-files-webpack-plugin/issues"
  },
  "homepage": "https://github.com/Barretem/unused-files-webpack-plugin/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "remove",
    "unused",
    "redundancy",
    "files",
    "require",
    "webpack",
    "webpack5",
    "webpack-plugin"
  ],
  "scripts": {
    "build": "rimraf dist & tsc",
    "dev": "tsc --watch",
    "lint": "eslint ./src --ext .js,.ts",
    "test": "vitest",
    "changeset": "changeset",
    "version": "changeset version",
    "publish": "npm run build && changeset publish",
    "webpack": "webpack --config ./example/webpack.config.js"
  },
  "peerDependencies": {
    "webpack": ">=5.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@types/fs-extra": "^11.0.4",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-xo": "^0.46.0",
    "eslint-plugin-no-explicit-type-exports": "^0.12.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.2",
    "memory-fs": "^0.5.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "npm run lint",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
